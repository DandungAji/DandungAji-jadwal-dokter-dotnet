@model List<DoctorScheduleApp.Models.DoctorSchedule>
<h2>Jadwal Dokter Spesialis RS Kartini</h2>
<table class="table">
    <thead>
        <tr>
            <th>Nama Dokter</th>
            <th>Spesialisasi</th>
            <th>Hari Praktik</th>
            <th>Jam Praktik</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model)
        {
            <tr>
                <td>@schedule.DoctorName</td>
                <td>@schedule.Specialization</td>
                <td>@schedule.PracticeDay</td>
                <td>@schedule.PracticeTime</td>
            </tr>
        }
    </tbody>
</table>